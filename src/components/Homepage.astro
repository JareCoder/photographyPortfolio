---
const { images, alt, urls, width, height } = Astro.props;
---
<div id="homepage" class="flex justify-center items-center" data-images={images} data-alt={alt} data-urls={urls} data-width={width} data-height={height}>
  <a>
    <img id="randomHomePageImage" class="h-[440px] sm:h-[600px] lg:h-[700px] xl:h-[1000px] object-contain w-full object-center" />
  </a>
   <slot />
   <script src="../scripts/homePage.js" />
</div>